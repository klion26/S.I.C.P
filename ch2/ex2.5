;;car 
(define (car z)
  (define (ite t ans)
    (if (= 0 (modulo t 2)) ;;t still has a 
        (ite (/ t 2) (+ ans 1))
         ans))
  (ite z 0))
;;cdr
(define (cdr z)
  (define (ite t ans)
    (if (= 0 (modulo t 3))
        (ite (/ t 3) (+ ans 1))
        ans))
  (ite z 0))